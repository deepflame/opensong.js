{{#this}}
{{#if header}}
<h2>{{human_header header}}</h2>
{{/if}}
{{#lines}}
<table>
	<tr class="chords">
		{{#chords}}
		<td>{{transpose this}}</td>
		{{/chords}}
	</tr>
	{{#lyrics}}
	<tr class='lyrics'>
		{{#this}}
		<td>{{this}}</td>
		{{/this}}
	</tr>
	{{/lyrics}}
</table>
{{#if comments}}
<div class"comments">
  {{comments}}
</div>
{{/if}}
{{/lines}}
{{/this}}
